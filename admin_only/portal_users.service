[Unit]
Description=portalusers gunicorn daemon
Requires=portal_users.socket
After=network.target

[Service]
User=beta_portal_user
Group=nginx
WorkingDirectory=/home/beta_portal_user/portal/userdb/portalusers/
ExecStart=/home/beta_portal_user/portal/userdb/portalusers/env/bin/gunicorn --workers 3 --bind unix:/var/run/portal_users.sock portalusers.wsgi:application --access-logfile -

[Install]
WantedBy=multi-user.target
